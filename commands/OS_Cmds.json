{
    "operations": [
    {
        "label": "change passwd (reboot)",
        "commands": [
            "echo -e 'root\nroot' | passwd root",
            "reboot"
        ]
    },
    {
        "label": "",
        "commands": [
            "passwd -d root",
            "reboot"
        ]
    },
    {
        "label": "get OS version",
        "commands": [
            "cat /etc/os_version"
        ]
    },
    {
        "label": "get MAC in flash",
        "commands": [
            "flash get HW_NIC1_ADDR | cut -d '=' -f 2"
        ]
    },
    {
        "label": "set MAC in flash",
        "commands": [
            "flash get HW_NIC1_ADDR | cut -d '=' -f 2",
            "flash set HW_NIC1_ADDR d01411b0ffff",
            "flash get HW_NIC1_ADDR | cut -d '=' -f 2"
        ]
    }
    ],
    "timeout_sec": 10
}